
<h2>Hikers:</h2>

<table>
<%@users.each do |user| %> 
<tr>
    <td><a href="/users/<%=user.slug%>"><%=user.trailname%></a> </td>
</tr>
<%end%>
</table><br><br>

<form action='/hikes/new' method="get">
<input type="submit" value="Post a New Hike">
</form>
<form action='/logout' method="get">
<input type="submit" value="Log Out">
</form>